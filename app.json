{
  "expo": {
    "name": "flashcards",
    "slug": "mobile-flashcards",
    "description": "Flashcards on your mobile",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/android-chrome-512x512.png",
    "notification": {
      "icon": "./assets/favicon-32x32.png"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "jerryfishzz.flashcards.exp"
    },
    "android": {
      "package": "jerryfishzz.flashcards.exp"
    }
  }
}